<script>

var states1 = ['Alabama', 'Alaska', 'Arizona', 'Arkansas', 'California',
'Colorado', 'Connecticut', 'Delaware', 'Florida', 'Georgia', 'Hawaii',
'Idaho', 'Illinois', 'Indiana', 'Iowa', 'Kansas', 'Kentucky', 'Louisiana',
'Maine', 'Maryland', 'Massachusetts', 'Michigan', 'Minnesota'
];
// constructs the suggestion engine
var states = new Bloodhound({
  datumTokenizer: Bloodhound.tokenizers.whitespace,
  queryTokenizer: Bloodhound.tokenizers.whitespace,
  // `states` is an array of state names defined in "The Basics"
  local: states1
});

 $('#autocomplete_form').typeahead({
  hint: true,
  highlight: true,
  minLength: 1
},
{
  name: 'states',
  source: states,
  callback: {
    onInit: function (node) {
      console.log('Typeahead Initiated on ' + node.selector);
  }
}
});
</script>




<div class="jumbotron text-center">
  <h1>Meet</h1>
  <p>make a party.</p>
  <form class="form-inline" id="search" action="/schedule/search" method="get">
    <div class="input-group">
      <input type="text" id="autocomplete_form" class="form-control" name="input" size="50" placeholder="Search city or tags"  required>
      <!-- class="form-control"-->
      <div class="input-group-btn">
        <button type="submit" class="btn btn-secondary" value ="search">Search</button>
      </div>
    </div>
  </form>
</div>
